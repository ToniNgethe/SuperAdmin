<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controlla.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDoctorServlet</servlet-name>
        <servlet-class>controlla.AddDoctorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPharmacist</servlet-name>
        <servlet-class>controlla.AddPharmacist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddTechnicianServlet</servlet-name>
        <servlet-class>controlla.AddTechnicianServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PopulateTableServlate</servlet-name>
        <servlet-class>controlla.PopulateTableServlate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UDoperations</servlet-name>
        <servlet-class>controlla.UDoperations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ROperations</servlet-name>
        <servlet-class>controlla.ROperations</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDoctorServlet</servlet-name>
        <url-pattern>/AddDoctorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPharmacist</servlet-name>
        <url-pattern>/AddPharmacist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddTechnicianServlet</servlet-name>
        <url-pattern>/AddTechnicianServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PopulateTableServlate</servlet-name>
        <url-pattern>/PopulateTableServlate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UDoperations</servlet-name>
        <url-pattern>/UDoperations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ROperations</servlet-name>
        <url-pattern>/ROperations</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
